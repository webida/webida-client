<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <category
            name="GEF Book"
            id="net.ajaxlab.gef">
      </category>
      <view
            name="Genealogy"
            icon="icons/sample.gif"
            category="net.ajaxlab.gef"
            class="net.ajaxlab.genealogy.view.GenealogyView"
            id="net.ajaxlab.genealogy.view.GenealogyView">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               ratio="0.5"
               relative="org.eclipse.ui.views.ProblemView"
               relationship="right"
               id="net.ajaxlab.genealogy.view.GenealogyView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>

</plugin>
